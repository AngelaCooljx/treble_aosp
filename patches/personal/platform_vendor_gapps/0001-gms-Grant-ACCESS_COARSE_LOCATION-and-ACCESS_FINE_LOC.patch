From 8d102907d42bbc4dfe8757a337c81780c0983667 Mon Sep 17 00:00:00 2001
From: Alberto Ponces <ponces26@gmail.com>
Date: Tue, 28 Feb 2023 21:15:27 +0000
Subject: [PATCH] gms: Grant ACCESS_COARSE_LOCATION and ACCESS_FINE_LOCATION

---
 .../etc/permissions/privapp-permissions-google-product.xml      | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/common/proprietary/product/etc/permissions/privapp-permissions-google-product.xml b/common/proprietary/product/etc/permissions/privapp-permissions-google-product.xml
index 02be696..00576f4 100644
--- a/common/proprietary/product/etc/permissions/privapp-permissions-google-product.xml
+++ b/common/proprietary/product/etc/permissions/privapp-permissions-google-product.xml
@@ -182,7 +182,9 @@
     <privapp-permissions package="com.google.android.gms">
         <permission name="android.permission.ACCESS_BROADCAST_RESPONSE_STATS"/>
         <permission name="android.permission.ACCESS_CACHE_FILESYSTEM"/>
+        <permission name="android.permission.ACCESS_COARSE_LOCATION"/>
         <permission name="android.permission.ACCESS_CONTEXT_HUB"/>
+        <permission name="android.permission.ACCESS_FINE_LOCATION"/>
         <permission name="android.permission.ACCESS_FPS_COUNTER"/>
         <permission name="android.permission.ACCESS_NETWORK_CONDITIONS"/>
         <permission name="android.permission.ACCESS_VIBRATOR_STATE"/>
